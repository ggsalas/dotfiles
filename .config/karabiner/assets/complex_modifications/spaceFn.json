{
  "title": "SpaceFN",
  "rules": [
    {
      "description": "[SpaceFN]: Space enables SpaceFN mode (see: https://geekhack.org/index.php?topic=51069.0)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "spacefn_mode",
                "value": 1
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "spacebar"
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "spacefn_mode",
                "value": 0
              }
            }
          ]
        }
      ]
    },
